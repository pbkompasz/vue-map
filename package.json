{
  "name": "vue-map",
  "private": true,
  "version": "1.0.0",
  "description": "A Map solution for Vue 3 built on Leaflet and GeoJSON",
  "keywords": [
    "vue",
    "vue3",
    "vuejs",
    "vue3js",
    "vue-component",
    "vue3-component",
    "map",
    "vue-map",
    "vue3-map",
    "vuemap"
  ],
  "scripts": {
    "dev": "vite",
