  },
  "main": "dist/vue-map.umd.js",
  "module": "dist/vue-map.es.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "author": {
    "name": "Peter-Bence Kompasz",
    "email": "kompaszbence@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pbkompasz/vue-map.git"
  },
  "dependencies": {
    "geojson": "^0.5.0",
    "geojson-validation": "^1.0.2",
    "leaflet": "^1.8.0"
  },
  "devDependencies": {
    "@types/geojson": "^7946.0.8",
    "@types/leaflet": "^1.7.10",
    "@vitejs/plugin-vue": "^2.3.3",
    "cypress": "^10.8.0",
    "sass": "^1.52.1",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vitest": "^0.23.4",
